/// @description controls_setKeys(num);
/// @function controls_setKeys
/// @param num
//// Set the global variables to each Key object!

var NUM     = argument0;
switch(NUM){
    case 0:
        global.Up = variable;
    break;
    case 1:
        global.Down     = variable;
    break;
    case 2:
        global.Left     = variable;
    break;
    case 3:
        global.Right    = variable;
    break;
    case 4:
        global.Action1  = variable;
    break;
    case 5:
        global.Action2  = variable;
    break;
    case 6:
        global.Jump     = variable
    break;
}


